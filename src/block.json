{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "block-developer-cookbook/iapi-gallery-slider",
	"version": "1.0.0",
	"title": "Gallery Slider",
	"category": "media",
	"icon": "media-interactive",
	"description": "A gallery slider built using the Interactivity API",
	"example": {},
	"supports": {
		"interactivity": true,
		"align": true,
		"alignWide": true
	},
	"attributes": {
		"continuous": {
			"type": "boolean",
			"default": false
		},
		"autoplay": {
			"type": "boolean",
			"default": false
		},
		"speed": {
			"type": "string",
			"default": "3"
		}
	},
	"textdomain": "iapi-gallery-slider",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"render": "file:./render.php",
	"style": "file:./style-index.css",
	"viewScriptModule": "file:./view.js"
}
